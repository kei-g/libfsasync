  "description": "Async fs Library",
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.7.1",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "mkdirp": "^1.0.4",
    "mocha": "^9.1.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "terser": "^5.7.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5",
    "uuid": "^8.3.2"
  },
  "displayName": "libfsasync",
  "files": [
    "CHANGELOG.md",
    "index.d.ts",
    "index.js",
    "lib"
  ],
  "keywords": [
    "async",
    "fs",
    "Promise"
  ],
  "homepage": "https://github.com/kei-g/libfsasync",
  "license": "BSD-3-Clause",
  "main": "index.js",
  "name": "libfsasync",
  "nyc": {
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "statements": 100
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kei-g/libfsasync.git"
  },
  "scripts": {
    "build": "run-s clean lint build:transpile-and-mkdir build:copy-and-terse",
    "build:copy": "run-p build:copy:*",
    "build:copy-and-terse": "run-p build:copy build:terse",
    "build:copy:index": "ts-node copy.ts build/\\*.d.ts ./",
    "build:copy:libraries": "ts-node copy.ts build/lib/\\*.d.ts lib/",
    "build:mkdir": "mkdirp lib",
    "build:terse": "run-p build:terse:*",
    "build:terse:fs-async": "terser build/lib/fs-async.js -c -m -o lib/fs-async.js --toplevel",
    "build:terse:index": "terser build/index.js -c -m -o index.js --toplevel",
    "build:transpile": "tsc",
    "build:transpile-and-mkdir": "run-p build:transpile build:mkdir",
    "clean": "rimraf build/ index.d.ts index.js lib/",
    "cover": "run-s cover:clean cover:check cover:clean",
    "cover:check": "nyc --check-coverage -r html -r text _mocha",
    "cover:clean": "rimraf tmp/",
    "lint": "eslint src/*.ts src/lib/*.ts",
    "postpublish": "run-s clean",
    "prepublishOnly": "run-s build",
    "start": "true",
    "test": "run-p cover lint"
  },
  "types": "lib/index.d.ts",
  "version": "1.0.1"
}
