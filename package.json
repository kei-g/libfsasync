    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "chai": "^4.3.4",
    "eslint": "^8.2.0",
    "mkdirp": "^1.0.4",
    "mocha": "^9.1.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "terser": "^5.9.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "displayName": "libfsasync",
  "keywords": [
    "Promise",
    "async",
    "fs",
    "library"
  ],
  "homepage": "https://github.com/kei-g/libfsasync",
  "license": "BSD-3-Clause",
  "main": "index.js",
  "name": "libfsasync",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kei-g/libfsasync.git"
  },
  "scripts": {
    "build": "npm-run-all -p clean lint -p build:mkdir build:tsc -p build:terse:*",
    "build:mkdir": "mkdirp lib",
    "build:terse:fs-async": "terser build/lib/fs-async.js -c -m -o lib/fs-async.js --toplevel",
    "build:terse:index": "terser build/index.js -c -m -o index.js --toplevel",
    "build:tsc": "tsc",
    "clean": "rimraf .nyc_output/ build/ coverage/ index.d.ts index.js lib/",
    "cover": "run-s cover:clean cover:check cover:clean",
    "cover:check": "nyc --check-coverage -r html -r text _mocha",
    "cover:clean": "rimraf tmp/",
    "lint": "eslint src/*.ts src/lib/*.ts",
    "postpublish": "run-s clean",
    "prepublishOnly": "run-s build",
    "start": "true",
    "test": "run-p cover lint"
  },
  "types": "lib/index.d.ts",
  "version": "1.0.2"
}
