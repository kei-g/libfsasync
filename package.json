  },
  "repository": "github:kei-g/libfsasync",
  "scripts": {
    "build": "npm-run-all -p clean lint -p build:mkdir build:tsc -p build:esbuild",
    "build:esbuild": "esbuild src/index.ts --bundle --minify --outfile=index.js --platform=node",
    "build:mkdir": "mkdirp lib",
    "build:tsc": "tsc",
    "clean": "rimraf .nyc_output/ build/ coverage/ index.d.ts index.js lib/",
    "cover": "run-s cover:clean cover:check cover:clean",
    "cover:check": "nyc --check-coverage -r html -r text _mocha",
    "cover:clean": "rimraf tmp/",
    "lint": "eslint src/**/*.ts",
    "postpublish": "run-s clean",
    "prepublishOnly": "run-s build",
    "start": "true",
    "test": "run-p cover lint"
  },
  "types": "index.d.ts",
  "version": "1.0.0"
}
